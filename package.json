    "@babel/preset-stage-0": "^7.0.0",
    "@babel/runtime": "^7.6.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "balloon-css": "^1.0.3",
    "classnames": "^2.2.6",
    "codemirror": "^5.49.2",
    "electron-debug": "^3.0.1",
    "electron-json-storage": "^4.1.8",
    "erb": "^1.3.0-hf.1",
    "electron-updater": "4.3.9",
    "es6-promisify": "^6.0.2",
    "fix-path": "^2.1.0",
    "fuse.js": "^3.4.5",
    "handlebars": "4.7.7",
    "immutable": "^4.0.0-rc.12",
    "js-beautify": "^1.10.2",
    "js-yaml": "^3.14.0",
    "mjml": "^4.12.0",
    "mjml-migrate": "^4.12.0",
    "ncp": "^2.0.0",
    "node-mailjet": "^3.3.1",
    "react": "^16.11.0",
    "react-collapse": "^5.0.0",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.15",
